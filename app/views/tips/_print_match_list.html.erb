<% logger.info "*********************  class = #{matches.class}"%>
<% if matches.class == Hash or matches.class == ActiveSupport::OrderedHash %>
  <% matches.each do |key, value| %>
    <h<%= level %>><%= key %></h<%= level %>>
    <%= render :partial => 'print_match_list', :locals => { :matches => value, :level => (level + 1) } %>
  <% end %>
<% else %>
  <% matches.each do |match| %>
    <%= render :partial => 'match_entry', :locals => { :match => match, :tip => @tips.where(:match_id => match.id).first } %>
  <% end %>
<% end %>